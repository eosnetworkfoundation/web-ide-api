{
  "name": "@overflow-ide/api",
  "author": "eosnetworkfoundation",
  "version": "0.0.1",
  "scripts": {
    "build": "npx ts-node build.ts",
    "start": "node -r module-alias/register ./dist --env=production",
    "dev": "nodemon",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.spec.ts' --exit --timeout 10000"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@src": "dist"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "dependencies": {
    "@greymass/eosio-resources": "^0.9.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "eosjs": "^22.1.0",
    "eosjs-ecc": "^4.0.7",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "inserturlparams": "^1.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jet-logger": "^1.3.1",
    "jet-validator": "^1.0.4",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.1",
    "rimraf": "^4.4.1",
    "socket.io": "^4.6.1",
    "ts-command-line-args": "^2.4.2",
    "uuidv4": "^6.2.13",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^4.3.7",
    "eslint": "^8.37.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.1",
    "jasmine": "^4.6.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.3"
  }
}
